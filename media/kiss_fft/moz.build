EXPORTS.kiss_fft += [
    'kiss_fft.h',
    'kiss_fftr.h',
]

SOURCES += [
    'kiss_fft.c',
    'kiss_fftr.c',
]

# kiss_fft causes OOM error with some 32bit versions of GCC when using -O2
if '64' not in CONFIG['OS_TEST']:
    if CONFIG['GNU_CC']:
        CFLAGS += ['-O1']

FINAL_LIBRARY = 'xul'
